        <META_000035 xml:lang="en">A definition created by the ontology author(s) or developer(s), as distinct from other definitions from external sources.</META_000035>
        <rdfs:label xml:lang="en">Local Definition</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/rrovetto/MetadataAnnotations/META_000035"/>
